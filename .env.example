# Deep Research Claude Environment Configuration

# API Configuration
API_PORT=3000
API_HOST=0.0.0.0

# Database Configuration
POSTGRES_DB=deep_research
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:5432/${POSTGRES_DB}

# Message Queue Configuration
RABBITMQ_USER=admin
RABBITMQ_PASS=your_secure_password_here
RABBITMQ_URL=amqp://${RABBITMQ_USER}:${RABBITMQ_PASS}@localhost:5672

# Redis Configuration
REDIS_URL=redis://localhost:6379

# ChromaDB Configuration
CHROMA_URL=http://localhost:8000

# Agent Configuration
MAX_CONCURRENT_AGENTS=50
AGENT_TIMEOUT_SECONDS=300

# Claude API Configuration (if using Claude API)
ANTHROPIC_API_KEY=your_api_key_here

# Monitoring
GRAFANA_PASSWORD=your_secure_password_here

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Feature Flags
ENABLE_TELEMETRY=false
ENABLE_DEBUG_MODE=false